# Key fingerprint
regexp=[\dA-F]{40}$|=(?:\s[\dA-F]{4}){5}\s{2}[\dA-F]{4}((?:\s[\dA-F]{4}){4})
colours=magenta, bold magenta
-
# Key Id (short or long)
regexp=/([\dA-F]{8,16})
colours=unchanged, bold magenta
-
# comments
regexp=\(([^)]+)\)
colours=dark white, cyan
-
# eMail
regexp=\s<([^>]+)>$
colours=dark white, bright_yellow
-
# Date
regexp=\s(\d{4}-\d{2}-\d{2})\s
colours=unchanged, bright_blue
-
# Expires
regexp=expires:\s(\d{4}-\d{2}-\d{2})
colours=unchanged, underline bold red
-
# Expired
regexp=expired:\s\d{4}-\d{2}-\d{2}
colours=red
-
# LEFT margin types
# secret key
regexp=^sec(#)?
colours=on_green black, bold red
-
# secret SuBkey
regexp=^ssb(#)?
colours=green, bold red
-
# public key
regexp=^pub
colours=cyan
-
# public subkey
regexp=^sub
colours=dark cyan
-
# TRUST VALUES
# ultimate, full
regexp=ultimate|full
colours=bold green
-
# unknown, undefined
regexp=unknown|undefined|undef|expired
colours=red
-
# marginal
regexp=marginal
colours=yellow
-
# never, revoked, err
regexp=never|revoked|err
colours=bold red
-
# dsa
regexp=dsa\d+
colours=red
-
# rsa4x
regexp=rsa[4]\d+
colours=green
-
# rsa2x
regexp=rsa[23]\d+
colours=yellow
-
# rsa1x
regexp=rsa[1]\d+
colours=red
-
# Elgamal
regexp=elg\d+
colours=red
-
# ed25519
regexp=(?:ed|cv)25519|nistp384|brainpoolP256r1
colours=green
-
# USAGE (E=encryption, S=signing, C=certification, A=authentication)
regexp=(?<=\[)[SEARC]*([E])
colours=unchanged, cyan
-
regexp=(?<=\[)[SEARC]*([S])
colours=unchanged, green
-
regexp=(?<=\[)[SEARC]*([C])
colours=unchanged, yellow
-
regexp=(?<=\[)[SEARC]*([A])
colours=unchanged, magenta
-
regexp=(?<=\[)[SEARC]*([R])
colours=unchanged, bold red

